{
  "manifest_version": 3,
  "name": "bringbackmaps",
  "version": "1.0",
  "description": "Add the 'Maps' tab back to Google search results",
  "content_scripts": [
    {
      "matches": ["*://www.google.com/*", "*://google.com/*"],
      "js": ["src/content.js"],
      "css": ["src/styles.css"]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
